# Google Cloud API Key (required for ADK)
GOOGLE_API_KEY="your_google_api_key_here"

# Remote Agent Addresses (comma-separated list)
REMOTE_AGENT_ADDRESSES=http://localhost:10003,http://localhost:10004

# Orchestrator Domain Configuration
# Options: 'security' (security-focused), 'general' (generic orchestration)
ORCHESTRATOR_DOMAIN=general

# Logging Configuration
# Log levels: DEBUG, INFO, WARNING, ERROR
ORCHESTRATOR_LOG_LEVEL=INFO
# Enable detailed diagnostic logging (true/false)
ORCHESTRATOR_DIAGNOSTIC_MODE=false

# Circuit Breaker Configuration (optional)
# Number of failures before opening circuit
CIRCUIT_BREAKER_FAILURE_THRESHOLD=3
# Seconds to wait before attempting recovery
CIRCUIT_BREAKER_RECOVERY_TIMEOUT=60

# Health Check Configuration (optional)
# Timeout for health checks in seconds
HEALTH_CHECK_TIMEOUT=5